{
  "configurations": [
    {
      "type": "mcuxpresso-debug",
      "name": "Debug project configuration",
      "request": "launch",
      "cwd": "${workspaceRoot}",
      "executable": {
        "elf": "${command:cmake.launchTargetPath}"
      },
      "gdbClientPath": "",
      "gdbServerTargetRemote": "",
      "gdbInitCommands": [
        "set remotetimeout 600",
        "set debug-file-directory"
      ],
      "stopAtSymbol": "main",
      "probeSerialNumber": "",
      "isAttach": false,
      "showDevDebugOutput": "none",
      "gdbServerConfigs": {
        "linkserver": {
          "device": "MIMXRT1176xxxxx:MIMXRT1170-EVK",
          "core": "cm7"
        },
        "segger": {
          "device": "MIMXRT1176xxxA_M7"
        }
      }
    }
  ]
}